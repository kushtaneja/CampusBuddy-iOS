# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'Campus Buddy' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

    pod 'SDWebImage', '~>3.8'
    pod 'SKPhotoBrowser', :git => 'https://github.com/suzuki-0000/SKPhotoBrowser.git', :branch => 'swift3'
    pod 'GoogleMaps'
    pod 'FacebookCore'
    pod 'FacebookLogin'
    pod 'FacebookShare'
    pod 'Fabric'
    pod 'Crashlytics'

  # Pods for Campus Buddy

  target 'Campus BuddyTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Campus BuddyUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
